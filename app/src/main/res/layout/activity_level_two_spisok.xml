<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    android:background="#D3FBFAFA"
    android:padding="@dimen/box_inset_layout_padding"
    tools:context=".level_one_spisok"
    tools:deviceIds="wear">


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="400dp"

            android:orientation="vertical">

            <Button
                android:id="@+id/btn_back"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="15dp"
                android:layout_marginEnd="10dp"
                android:background="@drawable/stule_btn_black95_and_true"

                android:gravity="center"
                android:text="Выход в главное меню"
                android:textColor="@color/text"
                android:textSize="24sp" />
            <TextView
                android:id="@+id/honesty_test"
                android:layout_width="match_parent"
                android:layout_height="67dp"
                android:layout_marginLeft="7dp"

                android:gravity="fill"
                android:background="#4A989696"
                android:layout_marginTop="30dp"
                android:text="Тест на внимательность"
                android:textColor="@color/text"
                android:textSize="23sp" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="match_parent"
                android:layout_height="67dp"
                android:layout_marginLeft="7dp"
                android:layout_marginTop="5dp"
                android:background="#25989696"
                android:gravity="fill"
                android:text="Можете ли вы быть предпринимателем?"
                android:textColor="@color/text"
                android:textSize="23sp" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="match_parent"
                android:layout_height="67dp"
                android:layout_marginLeft="7dp"
                android:layout_marginTop="5dp"
                android:gravity="fill"
                android:background="#4A989696"
                android:text="Добрый ли вы?"
                android:textColor="@color/text"
                android:textSize="23sp" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="match_parent"
                android:layout_height="67dp"
                android:gravity="fill"
                android:layout_marginLeft="7dp"
                android:layout_marginTop="5dp"
                android:background="#4A989696"
                android:text="Ваш темперамент"
                android:textColor="@color/text"
                android:textSize="23sp" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="match_parent"
                android:layout_height="67dp"
                android:layout_marginLeft="7dp"
                android:layout_marginTop="5dp"
                android:gravity="fill"
                android:background="#4A989696"
                android:text="Ваш психологический возраст"
                android:textColor="@color/text"
                android:textSize="23sp" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="match_parent"
                android:layout_height="67dp"
                android:layout_marginLeft="7dp"
                android:gravity="fill"
                android:layout_marginTop="5dp"
                android:background="#4A989696"
                android:text="Ваш творческий возраст"
                android:textColor="@color/text"
                android:textSize="23sp" />

            <TextView
                android:id="@+id/textView10"
                android:layout_width="match_parent"
                android:layout_height="67dp"
                android:layout_marginLeft="7dp"
                android:gravity="fill"
                android:layout_marginTop="5dp"
                android:background="#4A989696"
                android:text="Ваш творческий потенциал"
                android:textColor="@color/text"
                android:textSize="23sp" />

            <TextView
                android:id="@+id/textView11"
                android:layout_width="match_parent"
                android:layout_height="67dp"
                android:gravity="fill"
                android:layout_marginLeft="7dp"
                android:layout_marginTop="5dp"
                android:background="#4A989696"
                android:text="В порядке ли ваши нервы?"
                android:textColor="@color/text"
                android:textSize="23sp" />

            <TextView
                android:id="@+id/textView12"
                android:layout_width="match_parent"
                android:layout_height="67dp"
                android:layout_marginLeft="7dp"
                android:layout_marginTop="5dp"
                android:gravity="fill"
                android:background="#4A989696"
                android:text="Благоразумность"
                android:textColor="@color/text"
                android:textSize="23sp" />


        </LinearLayout>

    </ScrollView>

</RelativeLayout>